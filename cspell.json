{
  "version": "0.2",
  "ignorePaths": [
    "node_modules",
    "dist",
    "out",
    "logs",
    "e2e/playwright-report"
  ],
  "languageSettings": [
    {
      "languageId": "*",
      "locale": "en-US"
    }
  ],
  "dictionaries": [
    "npm",
    "node",
    "javascript",
    "typescript",
    "html",
    "css",
    "cspell-dictionary",
    "custom-dictionary"
  ],
  "dictionaryDefinitions": [
    {
      "name": "custom-dictionary",
      "path": "./.cspell/custom-dictionary.txt",
      "addWords": true
    }
  ],
  "words": ["taskguardian"],
  "ignoreWords": ["linebreak", "commitlint", "commitlintrc", "Mwingira"],
  "import": []
}
